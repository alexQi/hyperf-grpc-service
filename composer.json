{
  "name": "alex/hyperf-grpc-service",
  "description": "hyperf gprc service register to consul",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "alex",
      "email": "alex.qiubo@qq.com"
    }
  ],
  "require": {
    "php": ">=7.2",
    "google/protobuf": "^3.16",
    "hyperf/contract": "~2.1.0",
    "hyperf/http-message": "~2.1.0",
    "hyperf/load-balancer": "~2.1.0",
    "hyperf/rpc": "~2.1.0",
    "hyperf/service-governance": "^2.1",
    "hyperf/utils": "~2.1.0",
    "psr/container": "^1.0",
    "psr/log": "^1.0"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^2.14",
    "mockery/mockery": "^1.0",
    "phpstan/phpstan": "^0.12",
    "phpunit/phpunit": ">=7.0",
    "swoole/ide-helper": "dev-master",
    "swow/swow": "dev-develop",
    "symfony/var-dumper": "^5.1"
  },
  "autoload": {
    "psr-4": {
      "GrpcService\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "HyperfTest\\": "tests"
    }
  },
  "extra": {
    "hyperf": {
      "config": "GrpcService\\ConfigProvider"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true
  },
  "scripts": {
    "test": "phpunit -c phpunit.xml --colors=always",
    "analyse": "phpstan analyse --memory-limit 1024M -l 0 ./src",
    "cs-fix": "php-cs-fixer fix $1"
  }
}
